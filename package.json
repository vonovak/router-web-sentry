{
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "EXPO_UNSTABLE_ATLAS=true expo start --web -c",
    "deploy": "yarn export && npx eas-cli@latest deploy",
    "export": "npx expo export -p web && yarn sentry:sourcemaps",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org abc --project xyz ./dist && sentry-cli sourcemaps upload --org abc --project xyz ./dist"
  },
